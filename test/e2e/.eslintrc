{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:jasmine/recommended"
  ],
  "plugins": [
    "jasmine",
    "webdriverio"
  ],
  "env": {
    "webdriverio/wdio": true,
    "jasmine": true,
    "node": true,
    "es2020": true
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
