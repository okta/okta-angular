{
  "name": "@okta/okta-angular",
  "private": true,
  "version": "4.0.0",
  "description": "Angular support for Okta",
  "main": "./dist/bundles/okta-angular.umd.js",
  "module": "./dist/fesm5/okta-angular.js",
  "es2015_ivy_ngcc": "__ivy_ngcc__/dist/fesm2015/okta-angular.js",
  "es2015": "./dist/fesm2015/okta-angular.js",
  "esm5": "./dist/esm5/okta-angular.js",
  "esm2015": "./dist/esm2015/okta-angular.js",
  "fesm5": "./dist/fesm5/okta-angular.js",
  "fesm2015_ivy_ngcc": "__ivy_ngcc__/dist/fesm2015/okta-angular.js",
  "fesm2015": "./dist/fesm2015/okta-angular.js",
  "types": "./dist/okta-angular.d.ts",
  "scripts": {
    "banners": "node util/maintain-banners.js",
    "build:package-info": "node ./util/write-package-info.js . src/okta/packageInfo.ts",
    "clean": "rimraf dist/ && rimraf test-reports/",
    "lint": "eslint .",
    "lint:report": "eslint -f checkstyle -o ./test-reports/lint/eslint-checkstyle-result.xml .",
    "prebuild": "yarn build:package-info",
    "build": "node build.js",
    "prepare": "yarn build",
    "start": "yarn --cwd test/app start",
    "test": "yarn lint && yarn test:e2e && yarn test:unit",
    "test:e2e": "yarn workspace @okta/test.e2e test",
    "test:unit": "jest",
    "pack:lib": "npm run build && npm pack ./dist",
    "publish:lib": "npm run build && npm publish ./dist",
    "publish:lib:next": "npm run build && npm publish --tag next ./dist",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc:serve": "compodoc -s",
    "prepublishOnly": "node --eval \"console.error('ERROR: Trying to publish a package that has been compiled by NGCC. This is not allowed.\\nPlease delete and rebuild the package, without compiling with NGCC, before attempting to publish.\\nNote that NGCC may have been run by importing this package into another project that is being built with Ivy enabled.\\n')\" && exit 1"
  },
  "repository": "https://github.com/okta/okta-angular",
  "homepage": "https://github.com/okta/okta-angular",
  "keywords": [
    "okta",
    "oidc",
    "OpenID Connect",
    "authentication",
    "auth",
    "oauth2"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@okta/okta-auth-js": "^5.2.3",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@angular/animations": "11.2.10",
    "@angular/common": "11.2.10",
    "@angular/compiler": "11.2.10",
    "@angular/compiler-cli": "11.2.10",
    "@angular/cli": "11.2.9",
    "@angular/core": "11.2.10",
    "@angular/platform-browser": "11.2.10",
    "@angular/platform-browser-dynamic": "11.2.10",
    "@angular/platform-server": "11.2.10",
    "@angular/router": "11.2.10",
    "@compodoc/compodoc": "^1.1.11",
    "@rollup/plugin-commonjs": "11.0.1",
    "@rollup/plugin-node-resolve": "7.0.0",
    "@types/jest": "^26.0.14",
    "@types/json-schema": "^7.0.3",
    "@types/node": "10.14.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "angular2-template-loader": "0.6.2",
    "axios": "^0.21.0",
    "chalk": "2.4.2",
    "codelyzer": "^6.0.1",
    "core-js": "3.1.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-node": "^11.1.0",
    "globby": "^11.0.1",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "jest-preset-angular": "^8.3.1",
    "prettier": "^2.1.2",
    "raw-loader": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "^3.0.2",
    "rollup": "1.29.0",
    "rollup-plugin-license": "^2.3.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rxjs": "6.6.3",
    "shelljs": "0.8.3",
    "source-map-loader": "0.2.4",
    "to-string-loader": "1.1.5",
    "ts-loader": "^9.0.2",
    "tsickle": "^0.39.1",
    "tslint": "^6.1.3",
    "typescript": "~4.0.7",
    "uglify-js": "3.13.5",
    "webpack": "^4.32.2",
    "webpack-filter-warnings-plugin": "1.2.1",
    "zone.js": "~0.10.3"
  },
  "peerDependencies": {},
  "jest": {
    "coverageDirectory": "<rootDir>/test-reports/coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**",
      "!./test/**"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "@okta/okta-auth-js": "<rootDir>/node_modules/@okta/okta-auth-js/dist/okta-auth-js.umd.js"
    },
    "restoreMocks": true,
    "transform": {
      "^.+\\.(ts|html)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules",
      "packages/(?!okta-angular)"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/test/spec/tsconfig.spec.json"
      }
    },
    "preset": "jest-preset-angular",
    "roots": [
      "./test/spec"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/support/setupJest.ts"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./test-reports/unit/",
    "outputName": "junit-result.xml"
  },
  "sideEffects": false,
  "workspaces": {
    "nohoist": ["test/app"],
    "packages": [
      "./",
      "env",
      "test/e2e"
    ]
  },
  "__processed_by_ivy_ngcc__": {
    "es2015": "11.2.14",
    "fesm2015": "11.2.14",
    "typings": "11.2.14"
  }
}
